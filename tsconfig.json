{
    "compilerOptions": {
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "allowSyntheticDefaultImports": true,
        "noEmitHelpers": true,
        "importHelpers": true,
        "downlevelIteration": true,
        "target": "es5",
        "module": "esnext",
        "sourceMap": true,
        "moduleResolution": "node",
        "lib": [
            "dom",
            "es2015"
        ],
        "baseUrl": "./",
        "paths": {
            "rust-lib": [
                "./target/wasm32-unknown-unknown/release/rust_wasm_tests"
            ],
            "rust-mem": [
                "./target/wasm32-unknown-unknown/release/rust_wasm_tests_bg"
            ]
        },
    }
}